<configuration>
	<appSettings>
		<add key="MONGODB_URI" value="mongodb://localhost:27017/RumballHomeVideosApp">
	</appSettings>
	<system.webServer>
		<handlers>
			<add name="iisnode" path="server/server.js" verb="*" modules="iisnode" />
		</handlers>
		<iisnode      
		node_env="production"
		/>
		<rewrite>
			<rules>
				<rule name="sendToNode">
					<match url="/*" />
					<action type="Rewrite" url="server/server.js" />
				</rule>
			</rules>
		</rewrite>
	</system.webServer>
</configuration>