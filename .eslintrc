{
	"extends": "airbnb",
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"rules": {
		"linebreak-style": ["error", "windows"],
		"no-multiple-empty-lines":"warn",
		"comma-dangle":"off",
		"eol-last": "warn",
		"indent":"off",
		"one-var":"off",
		"object-curly-spacing":"off",
		"no-plusplus":"off",
		"no-param-reassign":"off",
		"brace-style":"off",
		"camelcase":"warn",
		"max-len":"warn",
		"radix":"warn",
		"no-underscore-dangle":"warn",
		"prefer-template":"warn",
		"no-tabs":"off",
		"no-console":"off",
		"no-unused-vars": ["warn", {
			"vars": "local",
			"args": "none"
		}]
	}
}